<li>
  <h3>Accommodates</h3>
  <select class="accommodates" name="accommodates">
    <% for(var i = 1; i <= 16; i++) { %>
      <option value="<%= i %>"><%= i %></option>
    <% } %>
  </select>
</li>
<li>
  <h3>Room Type</h3>
    <input type="checkbox" name="room_type" value="Entire home/apt" checked>Entire place
    <input type="checkbox" name="room_type" value="Private room" checked>Private room
    <input type="checkbox" name="room_type" value="Shared room" checked>Shared room
</li>
<li>
  <p>
    <label for="amount">Price range: </label>
    <input type="text" class="amount" id="amount" readonly>
  </p>
  <div class="price-slider"></div>
</li>
